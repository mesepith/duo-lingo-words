{
  "manifest_version": 3,
  "name": "DuoLingo Word Extractor",
  "version": "1.2",
  "description": "Extract words and meanings from Duolingo and view them in a simple table",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/icon/icon_16x16.png",
      "48": "/icon/icon_48x48.png",
      "128": "/icon/icon_128x128.png"
    }
  }
}

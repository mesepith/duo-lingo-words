{
  "manifest_version": 3,
  "name": "Duo Lingo Word Extractor",
  "version": "1.0",
  "description": "Extract Spanish words and their meanings from a page and display them in a table.",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/icon/icon_16x16.png",
      "48": "/icon/icon_48x48.png",
      "128": "/icon/icon_128x128.png"
    }
  }
}
